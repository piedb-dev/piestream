statement ok
SET RW_IMPLICIT_FLUSH TO true;

statement ok
create table t(x int, y int);

statement ok
insert into t values 
    (1, 1), (1, 2), (1, 3), (1, 4), (1, 5),
    (2, 1), (2, 2), (2, 3), (2, 4), (2, 5),
    (3, 1), (3, 2), (3, 3), (3, 4), (3, 5),
    -- ties
    (1, 3), (2, 3), (3, 3);

query II rowsort
select x, y from (
    select *, ROW_NUMBER() OVER (PARTITION BY x ORDER BY y) as rank from t
)
where rank <= 3;
