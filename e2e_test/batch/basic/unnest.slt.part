# multiple table functions: padding with NULL
query I
select unnest(array[1,2,3]), unnest(array['a', 'b']);
----
1 a
2 b
3 NULL

query I
select unnest(array[1,2,3]) * unnest(array[10, 100]);
----
10
200
NULL

